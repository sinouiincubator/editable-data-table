{
  "name": "@sinouiincubator/editable-data-table",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/sinouiincubator-editable-data-table.umd.production.js",
  "module": "dist/sinouiincubator-editable-data-table.es.production.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "ts-lib-tools watch",
    "test": "ts-lib-tools test",
    "build": "ts-lib-tools build",
    "format": "ts-lib-tools format",
    "lint": "ts-lib-tools lint",
    "prepublishOnly": "ts-lib-tools build",
    "doc:dev": "docz dev",
    "doc:build": "docz build",
    "doc:publish": "docz dev && gh-pages -d .docz/dist"
  },
  "prettier": {
    "printWidth": 80,
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always"
  },
  "eslintConfig": {
    "extends": "ts-lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{ts,tsx}": [
      "cross-env CI=true ts-lib-tools test",
      "eslint --fix --color",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "files": [
    "dist",
    "assets"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.5.1",
    "@sinoui/use-rest-list-api": "^0.2.0",
    "classnames": "^2.2.6",
    "sinoui-components": "^0.1.22",
    "styled-components": "^3.4.10"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.4",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "css-loader": "^3.0.0",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "gh-pages": "^2.0.1",
    "husky": "^3.0.0",
    "jest-dom": "^3.5.0",
    "lint-staged": "^9.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "style-loader": "^0.23.1",
    "ts-lib-tools": "^0.2.7",
    "typescript": "^3.5.2"
  }
}
